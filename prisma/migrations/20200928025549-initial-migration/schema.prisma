// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id Int @id @default(autoincrement())
  email String 
  name String
  image String?
  jobTitle String?
}

model Post {
  id Int @id @default(autoincrement())
  question String
  description String
  language String?
  rating Int? 
}